{
  "name": "my-360-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run data:prepare && next build",
    "start": "next start",
    "lint": "next lint",
    "export": "npm run data:prepare && next build",
    "deploy": "npm run export && gh-pages -d out",
    "convert:webp": "node convert-to-webp.js",
    "optimize:images": "npm run convert:webp",
    "data:split": "ts-node --project scripts/tsconfig.json scripts/split-houses.ts",
